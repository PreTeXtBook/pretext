    <localization string-id='previous-short'>Vorig</localization>
    <localization string-id='up-short'>Op</localization>
    <localization string-id='next-short'>Volg</localization>
    <!-- NB: Use toc from above for both headings and navigation sidebar-->
    <localization string-id='annotations'>Aantekeninge</localization>
    <localization string-id='feedback'>Terugvoer</localization>
    <!-- This phrase should suggest that PreTeXt is the source -->
    <!-- language that makes a particular output possible      -->
    <localization string-id='authored'>Geskryf in</localization>
    <!-- Parts of memos and letters -->
    <localization string-id='to'>Aan</localization>
    <localization string-id='from'>Van</localization>
    <localization string-id='subject'>Onderwerp</localization>
    <localization string-id='date'>Datum</localization>
    <localization string-id='copy'>cc</localization>
    <localization string-id='enclosure'>bylae</localization>
    <!-- Various -->
    <localization string-id='todo'>Om Te Doen</localization>
    <localization string-id='editor'>Redakteur</localization>
    <localization string-id='edition'>Uitgawe</localization>
