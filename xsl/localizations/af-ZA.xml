    <localization string-id='me'>Vergelyking</localization>
    <localization string-id='men'>Vergelyking</localization>
    <localization string-id='mrow'>Vergelyking</localization>
    <!-- Display Mathematics -->
    <localization string-id='md'>Vertoon Wiskunde</localization>
    <localization string-id='mdn'>Vertoon Wiskunde</localization>
    <!-- Types of documents, mostly for informational messages -->
    <localization string-id='volume'>Volume</localization>
    <localization string-id='book'>Boek</localization>
    <localization string-id='article'>Artikel</localization>
    <localization string-id='slideshow'>Skyfie Vertoning</localization>
    <localization string-id='letter'>Brief</localization>
    <localization string-id='memo'>Memo</localization>
    <localization string-id='presentation'>Aanbieding</localization>
    <!-- Parts of a document -->
    <!-- "part" will also be used for a "stage" of a WeBWorK problem -->
    <localization string-id='frontmatter'>Voorsake</localization> <!-- Can't find this anywhere, so I made it up. -->
    <localization string-id='part'>Deel</localization>
    <localization string-id='chapter'>Hoofstuk</localization>
    <localization string-id='appendix'>Bylaag</localization>
    <!-- Next is the plural of Appendix, used to identify a group of appendix -->
    <!-- <localization string-id='appendices'>Appendices</localization> -->
    <localization string-id='section'>Afdeling</localization>
    <localization string-id='subsection'>Onderafdeling</localization>
    <localization string-id='subsubsection'>Onderonderafdeling</localization>
    <!-- A "slide" is a screenful of a presentation (Powerpoint, Beamer) -->
    <localization string-id='slide'>Skyfie</localization>
    <localization string-id='introduction'>Inleiding</localization>
    <localization string-id='conclusion'>Slot</localization>
    <localization string-id='exercises'>Oefeninge</localization>
    <localization string-id='worksheet'>Werkblad</localization>
    <localization string-id='reading-questions'>Leesvrae</localization>
    <localization string-id='solutions'>Oplossings</localization>
    <localization string-id='glossary'>Woordelys</localization>
    <localization string-id='references'>Verwysings</localization>
    <localization string-id='backmatter'>Nasake</localization> <!-- Can't find this anywhere, so I made it up. -->
    <!-- paragraph is deprecated, getting plural correct is not super critical, just in messages -->
    <localization string-id='paragraphs'>Paragrawe</localization>
    <localization string-id='commentary'>Kommentaar</localization>
    <localization string-id='subparagraph'>Onderparagrawe</localization>
    <!-- Components of the narrative -->
    <!-- REMARK-LIKE blocks -->
    <!-- "note" is used within "biblio", likely to change -->
    <localization string-id='remark'>Opmerking</localization>
    <localization string-id='convention'>Konvensie</localization>
    <localization string-id='note'>Nota</localization>
