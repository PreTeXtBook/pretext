    <localization string-id='computation'>Berekening</localization>
    <localization string-id='technology'>Tegnologie</localization>
    <!-- ASIDE-LIKE blocks -->
    <localization string-id='aside'>Ter Syde</localization>
    <localization string-id='biographical'>Biografiese Ter Syde</localization>
    <localization string-id='historical'>Geskiedkundige Ter Syde</localization>
